    "section-main": {
        "headline": {
            "titlecaption": "Corona-Warn-App Open Source Project",
            "title": "Thank you for using the Corona-Warn-App!"
        },
        "content": {
            "text": "The Corona-Warn-App was an app that helped tracing infection chains of SARS-CoV-2 (which can cause COVID-19) in Germany. The app was based on technologies with a decentralized approach and notified users if they had been exposed to SARS-CoV-2. Transparency is key to both protect the app's end-users and to encourage adoption, and that's why this website continues to be available, even after the shutdown of the app itself on xx.xx.xxxx.",
            "button": {
                "title": "GitHub project",
                "url": "https://github.com/corona-warn-app",
                "external": true,
                "class": "btn-secondary mr-3 my-2"
            }
        },
        "image": {
            "url": "/assets/img/app-mockup@2x.png",
            "url-webp": "/assets/img/app-mockup@2x.webp",
            "title": "Corona-Warn-App",
            "alt": "Corona-Warn-App Smartphone"
        }
    },
    "section-boxes": {
        "headline": {
            "title": "Latest blog posts"
        },
        "slider": {
